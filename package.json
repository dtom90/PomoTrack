{
  "name": "pomotrack",
  "displayName": "PomoTrack",
  "homepage": "https://github.com/dtom90/PomoTrack",
  "repository": "https://github.com/dtom90/PomoTrack",
  "version": "0.16.0",
  "private": true,
  "description": "An app for productive developers",
  "author": {
    "name": "David Thomason",
    "url": "https://davidthomason.com/"
  },
  "scripts": {
    "packages:audit": "/bin/bash -c 'yarn audit --groups dependencies; [[ $? -ge 8 ]] && exit 1 || exit 0'",
    "packages:fix": "npm_config_yes=true npx yarn-audit-fix",
    "lint": "vue-cli-service lint src/ cypress/ --no-fix",
    "lint:fix": "vue-cli-service lint src/ cypress/ --fix",
    "test:basic": "yarn packages:audit && yarn run lint",
    "web:dev": "vue-cli-service serve",
    "web:build": "vue-cli-service build --dest dist_web",
    "web:build:analyze": "ANALYZE_WEBPACK=true vue-cli-service build --dest dist_web",
    "web:serve": "node web.js",
    "web:prod": "yarn run web:build && yarn run web:serve",
    "cy:open": "cypress open",
    "cy:run": "cypress run -b chrome",
    "test:prod:build:e2e": "yarn run web:dev && yarn run cy:run",
    "electron:dev": "vue-cli-service electron:serve",
    "electron:build:mac": "CSC_IDENTITY_AUTO_DISCOVERY=false vue-cli-service electron:build --dir --mac",
    "electron:build:win": "vue-cli-service electron:build --dir --windows",
    "electron:release:mac": "vue-cli-service electron:build --mac",
    "electron:release:win": "vue-cli-service electron:build --windows",
    "electron:serve": "electron dist_electron/bundled",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "@babel/preset-env": "^7.26.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "babel-loader": "^9.2.1",
    "bootstrap": "^4.6.2",
    "bootstrap-vue": "^2.23.1",
    "chart.js": "^4.4.7",
    "chartjs-plugin-annotation": "^3.1.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "color-manager": "https://github.com/dtom90/color-manager",
    "core-js": "^3.38.1",
    "dayjs": "^1.11.13",
    "dexie": "^4.0.10",
    "dompurify": "^3.1.3",
    "electron-log": "^4.2.4",
    "electron-updater": "^6.3.9",
    "humanize-duration": "^3.32.1",
    "jquery": "^3.7.1",
    "lodash.clonedeep": "^4.5.0",
    "marked": "^4.0.10",
    "nanoid": "^3.3.8",
    "popper.js": "^1.16.0",
    "sass": "^1.80.4",
    "sass-loader": "^9.0.1",
    "vue": "^2.7.16",
    "vue-chartjs": "^5.3.2",
    "vue-color": "^2.7.0",
    "vue-ctk-date-time-picker": "^2.5.0",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.1.1",
    "vuex-persist": "^2.0.1"
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.2.5",
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@electron/notarize": "^2.5.0",
    "@testing-library/jest-dom": "5.11.9",
    "@vue/cli-plugin-babel": "^4.5.19",
    "@vue/cli-plugin-eslint": "^4.5.19",
    "@vue/cli-service": "^4.5.19",
    "@vue/test-utils": "^1.3.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "cypress": "^13.17.0",
    "dotenv": "^8.2.0",
    "electron": "^33.2.0",
    "electron-builder": "^23.0.2",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-chai-friendly": "^1.0.1",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "express": "^4.21.1",
    "fake-indexeddb": "^4.0.1",
    "moment": "^2.29.4",
    "sinon": "^19.0.2",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-template-compiler": "^2.7.16",
    "webpack": "^4.46.0",
    "vue-loader": "^15.9.8",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "resolutions": {
    "moment": ">=2.29.4",
    "@achrinza/node-ipc": ">=9.2.9"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
